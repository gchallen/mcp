{
  "name": "mcp-server-everything",
  "version": "0.1.0",
  "description": "Example MCP Server",
  "type": "module",
  "main": "dist/index.js",
  "engines": {
    "node": ">=22.0.0 <24.0.0"
  },
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsx watch --inspect src/index.ts",
    "dev:break": "tsx --inspect-brk watch src/index.ts",
    "build": "tsc && npm run copy-static",
    "copy-static": "mkdir -p dist/static && cp -r src/static/* dist/static/",
    "lint": "eslint src/",
    "test": "NODE_ENV=test NODE_OPTIONS='--experimental-vm-modules --disable-warning=ExperimentalWarning' jest"
  },
  "devDependencies": {
    "@eslint/js": "9.33.0",
    "@types/content-type": "1.1.9",
    "@types/cors": "2.8.19",
    "@types/express": "5.0.3",
    "@types/jest": "30.0.0",
    "@types/node": "24.3.0",
    "jest": "30.0.5",
    "ts-jest": "29.4.1",
    "tsx": "4.20.4",
    "typescript": "5.9.2",
    "typescript-eslint": "8.40.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "1.17.3",
    "@redis/client": "5.8.1",
    "cors": "2.8.5",
    "dotenv": "17.2.1",
    "express": "5.1.0",
    "raw-body": "3.0.0"
  },
  "overrides": {
    "@types/express": "5.0.3",
    "@types/express-serve-static-core": "5.0.2"
  }
}
